---
# *** WARNING ***
# This file is automatically generated by build.yml.
# Changes made to this file may be overwritten.
#

{% if summary_file is defined %}summary_file: {{ summary_file }}{% endif %}

{% if util_fqdn is defined %}util_fqdn: '{{ util_fqdn }}'{% endif %}

ssh_rsa:
  key:
{% for key in ssh_key %}
    - "{{ key }}"
{% endfor %}

ntp_server:
{% for server in ntp_server_list %}
  - {{ server }}
{% endfor %}
dns:
  servers:
{% for server in dns_server_list %}
    - {{ server }}
{% endfor %}
domain: {{ dns_domain }}

{% if files_location is defined %}files_location: '{{ files_location }}'
{% endif %}
{% if nuage_vrs_rpm_location is defined %}nuage_vrs_rpm_location: {{ nuage_vrs_rpm_location }}{% endif %} 
{% if nuage_libnetwork_rpm_location is defined %}nuage_libnetwork_rpm_location: {{ nuage_libnetwork_rpm_location }}{% endif %}
